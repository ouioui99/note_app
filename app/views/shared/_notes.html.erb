<% @notes.each do |note| %>
<div class="accordion" id="accordion<%=note.id%>">
  <div class="accordion-item w-25 ">
    <h2 class="accordion-header" id="heading<%=note.id%>">
      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse<%=note.id%>" aria-expanded="true" aria-controls="collapse<%=note.id%>">
        <%=note.title%>
      </button>
    </h2>
    <div id="collapse<%=note.id%>" class="accordion-collapse collapse toggle" aria-labelledby="heading<%=note.id%>" data-bs-parent="#accordion<%=note.id%>">
      <div class="accordion-body text-dark">
        <%note.content.each do |note_content|%>
        <%= note_content.contents %>
        <% end %>
      </div>
    </div>
  </div>
<% end %>